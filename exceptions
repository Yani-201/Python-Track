# Enter your code here. Read input from STDIN. Print output to STDOUT
if __name__ =="__main__":
    a=int(input())
    for i in range(a):
        x=input().split()
        try:
            print(int(x[0])//int(x[1]))
        except ZeroDivisionError as e:
            print("Error Code:",e);
        
        except ValueError as v:
            print("Error Code:", v)
